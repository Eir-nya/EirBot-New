<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
    <RootNamespace>EirBot_New</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DisCatSharp" Version="10.4.2-nightly-048" />
    <PackageReference Include="DisCatSharp.Analyzer.Roselyn" Version="6.1.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="DisCatSharp.ApplicationCommands" Version="10.4.2-nightly-048" />
    <PackageReference Include="DisCatSharp.Common" Version="10.4.2-nightly-048" />
    <PackageReference Include="DisCatSharp.Interactivity" Version="10.4.2-nightly-048" />
  </ItemGroup>

</Project>
